{
  "name": "appseed",
  "version": "1.0.8",
  "description": "Starter Kit for ES6 workflow",
  "main": "server/index.js",
  "scripts": {
    "prestart": "npm i",
    "start": "npm-run-all --parallel message:running-production serve:production server:rest-production",
    "message:running-production": "node_modules/.bin/babel-node message-running-production",
    "serve:production": "node_modules/.bin/live-server ./www",
    "server:rest-production": "cross-env NODE_ENV=production node_modules/.bin/babel-node ./server"
  },
 "author": "Phil Willis <pdotwdot@gmail.com> (http://www.philwillis.pw/)",
  "license": "ISC",
  "dependencies": {
    "cross-env": "3.2.4",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-preset-env": "1.2.2",
    "babel-register": "6.23.0",
    "babel-preset-airbnb": "2.4.0",
    "babel-preset-react": "6.24.1",
    "body-parser": "1.17.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "express": "4.15.0",
    "redux": "3.6.0",
    "live-server": "1.2.0",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "nsp": "2.6.2",
    "run-sequence": "1.2.2"
  },
   "nyc": {
    "include": [
      "www/js/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false
  }
}