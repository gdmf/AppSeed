{
  "name": "appseed-es6",
  "version": "1.0.0",
  "description": "Starter Kit for ES6 workflow",
  "main": "index.js",
  "scripts": {
    "up": "bower i && yarn",
    "start": "npm-run-all --parallel security-check test:watch server:dev bower bower:watch open-browser",
    "start:production": "npm-run-all message:running-production server:production open-browser",
    "test": "mocha --reporter spec tools/test-setup.js \"tests/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "pretest-report": "babel-node tools/test-reporter-clean",
    "test-report": "mocha --reporter mochawesome --reporter-options reportDir=tests/reports,reportFilename=frontend-tests-report,inlineAssets=true,autoOpen=true tools/test-setup.js 'tests/**/*.test.js'",
    "message:running-production": "babel-node tools/message-running-production",
    "open-browser": "babel-node tools/open-browser",
    "security-check": "nsp check",
    "bower": "babel-node tools/bower",
    "bower:watch": "node_modules/.bin/onchange 'bower.json' -v -- npm run bower",
    "bower:build": "babel-node tools/build-bower",
    "serve": "NODE_ENV=dev nodemon ./server --exec babel-node",
    "server:dev": "NODE_ENV=dev nodemon ./server --exec babel-node",
    "server:production": "NODE_ENV=production babel-node ./server",
    "webpack": "node_modules/.bin/webpack --config webpack.config.dev.js",
    "webpack:watch": "node_modules/.bin/webpack --config webpack.config.dev.js --watch",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "copy-assets": "babel-node tools/copy-assets",
    "inject-prod-tags": "babel-node tools/inject-prod-tags",
    "prebuild": "npm-run-all clean-dist test-report bower:build inject-prod-tags copy-assets",
    "build": "babel-node tools/build",
    "postbuild": "npm-run-all --parallel bower start:production open-browser"
  },
  "author": "Phil Willis <pdotwdot@gmail.com> (http://www.philwillis.pw/)",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.17.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "express": "4.15.0"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-preset-env": "^1.2.2",
    "babel-register": "6.23.0",
    "bower-main": "^0.2.14",
    "bower-webpack-plugin": "^0.1.9",
    "chai": "3.5.0",
    "cheerio": "^0.22.0",
    "cli-spinner": "0.2.6",
    "concat-files": "^0.1.1",
    "css-loader": "0.26.2",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-plugin-import": "2.2.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^0.10.1",
    "graceful-fs": "^4.1.11",
    "gulp": "3.9.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-real-favicon": "^0.2.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "main-bower-files": "^2.13.1",
    "merge2": "^1.0.3",
    "minimatch": "^3.0.2",
    "mocha": "3.2.0",
    "mochawesome": "2.0.4",
    "mochawesome-report": "1.0.0-alpha.1",
    "node-fs-extra": "^0.8.1",
    "node-sass": "^4.5.0",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "nsp": "2.6.2",
    "onchange": "^3.2.1",
    "open": "0.0.5",
    "postcss-loader": "^1.3.3",
    "rimraf": "2.6.1",
    "run-sequence": "1.2.2",
    "sass-loader": "4.1.1",
    "strip-comments": "^0.4.4",
    "style-loader": "0.13.2",
    "uglify-js": "^2.8.12",
    "uglifycss": "^0.0.25",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  }
}