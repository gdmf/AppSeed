{
  "name": "appseed",
  "version": "1.2.0",
  "description": "Starter Kit for ES6 workflow",
  "main": "server/index.js",
  "scripts": {
    "up": "bower i && npm i",
    "new:view": "node_modules/.bin/babel-node tools/new-view",
    "new:component": "node_modules/.bin/babel-node tools/new-component",
    "start": "npm-run-all security-check up bower:tags test:reporter-check",
    "poststart": "npm-run-all --parallel server:rest webpack:dev-server bower:watch test open-browser storybook",
    "app": "npm-run-all security-check bower:tags test:reporter-check",
    "postapp": "npm-run-all --parallel server:rest webpack:dev-server bower:watch test storybook",
    "storybook": "start-storybook -p 8083 -c .storybook ",
    "open-browser": "node_modules/.bin/babel-node tools/open-browser",
    "security-check": "nsp check",
    "bower:tags": "node_modules/.bin/babel-node tools/bower-tags",
    "bower:watch": "node_modules/.bin/onchange 'bower.json' -v -- npm run bower",
    "clean-dist": "node_modules/.bin/babel-node tools/build-clean",
    "clean-post": "node_modules/.bin/babel-node tools/build-post-clean",
    "copy-assets": "node_modules/.bin/babel-node tools/copy-assets",
    "build-dev-server": "node_modules/.bin/babel-node tools/build-dev-server",
    "build:files": "node_modules/.bin/babel-node tools/build-files",
    "build:inject-tags": "node_modules/.bin/babel-node tools/build-inject-tags",
    "build:web-config": "node_modules/.bin/babel-node tools/build-web-config",
    "prebuild": "npm-run-all clean-dist build:files build:inject-tags copy-assets",
    "build": "node_modules/.bin/babel-node tools/build-webpack",
    "postbuild": "npm-run-all bower:tags build-dev-server babel-server clean-post",
    "prebuild:azure": "npm-run-all clean-dist build:files build:inject-tags copy-assets",
    "build:azure": "node_modules/.bin/babel-node tools/build-webpack",
    "postbuild:azure": "npm-run-all bower:tags build-azure-server build:web-config babel-server-azure clean-post",
    "build-azure-server": "node_modules/.bin/babel-node tools/build-azure-server",
    "babel-server": "node_modules/.bin/babel DEPLOY/nodejs/server --out-dir DEPLOY/nodejs/server",
    "babel-server-azure": "node_modules/.bin/babel DEPLOY/server --out-dir DEPLOY/server",
    "server:rest": "cross-env NODE_ENV=dev nodemon ./server --exec babel-node",
    "webpack": "node_modules/.bin/webpack --config webpack.config.dev.js",
    "webpack:watch": "node_modules/.bin/webpack --config webpack.config.dev.js --watch",
    "webpack:dev-server": "node_modules/.bin/webpack-dev-server --config webpack.config.dev.js --watch",
    "deploy:init": "cd DEPLOY/ansible && ansible-playbook init.yml",
    "deploy": "cd DEPLOY/ansible && ansible-playbook update.yml",
    "test": "npm-run-all --parallel test:report-watch test:server",
    "test:watch": "cross-env NODE_ENV=test mocha-webpack -w --require tests/www/__spec-helper__.js --webpack-config webpack.config.test.js \"tests/www/**/*.test.js\"",
    "test:once": "mocha-webpack --require tests/www/__spec-helper__.js --webpack-config webpack.config.test.js \"tests/www/**/*.test.js\"",
    "test:report": "cross-env NODE_ENV=test mocha-webpack --require tests/www/__spec-helper__.js --webpack-config webpack.config.test.js \"tests/www/**/*.test.js\" --reporter mocha-simple-html-reporter --reporter-options output=tests-reports/index.html",
    "test:report-watch": "cross-env NODE_ENV=test mocha-webpack -w --require tests/www/__spec-helper__.js --webpack-config webpack.config.test.js \"tests/www/**/*.test.js\" --reporter mocha-simple-html-reporter --reporter-options output=tests-reports/index.html",
    "test:server": "cross-env NODE_ENV=test node_modules/.bin/babel-node tools/test-dev-server",
    "test:cover": "cross-env NODE_ENV=coverage nyc --reporter=lcov --reporter=text cross-env NODE_ENV=coverage npm run test:once",
    "test:reporter-check": "node_modules/.bin/babel-node tools/test-reporter-check",
    "test:jest": "jest",
    "test:jest-watch": "jest --watch --coverage",
    "test:react-coverage": "jest --coverage"
  },
  "author": "Phil Willis <pdotwdot@gmail.com> (http://www.philwillis.pw/)",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "chalk": "^2.1.0",
    "compression": "^1.7.1",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "install": "^0.10.1",
    "minimatch": "^3.0.4",
    "nodemon": "^1.12.1",
    "npm": "^5.5.1",
    "nsp": "^2.8.1"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.2.0",
    "@storybook/addon-knobs": "^3.2.0",
    "@storybook/react": "^3.2.3",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.0.2",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.23.0",
    "bower-main": "^0.2.14",
    "chai": "^3.5.0",
    "chmod": "^0.2.1",
    "concat-files": "^0.1.1",
    "cross-env": "^3.2.4",
    "css-loader": "^0.28.0",
    "enzyme": "^2.9.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "event-source-polyfill": "^0.0.9",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "gulp": "^3.9.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-real-favicon": "^0.2.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^1.2.0",
    "jest": "^21.1.0",
    "jest-html-reporter": "^0.5.1",
    "jest-styled-components": "^4.6.0",
    "jsdom": "^9.9.1",
    "live-server": "^1.2.0",
    "main-bower-files": "^2.13.1",
    "material-ui": "^0.19.1",
    "merge2": "^1.0.3",
    "mocha": "^3.2.0",
    "mocha-loader": "^1.1.1",
    "mocha-simple-html-reporter": "^1.0.1",
    "mocha-webpack": "^1.0.0-beta.1",
    "mochawesome": "^2.0.4",
    "mochawesome-report": "^1.0.0-alpha.1",
    "node-fs-extra": "^0.8.1",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.0.0",
    "onchange": "^3.2.1",
    "open": "^0.0.5",
    "ora": "^1.3.0",
    "postcss-loader": "^1.3.3",
    "prettier-eslint": "^8.2.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.0",
    "rimraf": "^2.6.1",
    "run-sequence": "^1.2.2",
    "sass-loader": "^4.1.1",
    "script-loader": "^0.7.0",
    "shelljs": "^0.7.7",
    "strip-comments": "^0.4.4",
    "style-loader": "^0.13.2",
    "styled-components": "^2.1.1",
    "uglifycss": "^0.0.25",
    "uppercamelcase": "^3.0.0",
    "webpack": "^2.7.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-node-externals": "^1.6.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "nyc": {
    "include": [
      "www/js/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "jest": {
    "roots": [
      "www/react"
    ],
    "testResultsProcessor": "./node_modules/jest-html-reporter"
  },
  "jest-html-reporter": {
    "pageTitle": "React Test Report",
    "outputPath": "tests-reports/react-coverage.html",
    "styleOverridePath": "tools/react-coverage.css"
  }
}
